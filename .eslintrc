{
    "extends": "airbnb",
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "script",
      "ecmaFeatures": {
        "modules": false
      }
    },
    "rules": {
      "strict": [2, "safe"],
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "generator-star-spacing": 0
    }
}
